                     data-widget_type="heading.default">
                     <div class="elementor-widget-container">
                        <h2 class="job-page-h2 elementor-heading-title elementor-size-default" id="about">People and
                           Culture Operations Specialist
                        </h2>
                     </div>
                  </div>
                  <div
                     class="elementor-element elementor-element-80285bd elementor-invisible elementor-widget elementor-widget-divider"
                     data-id="80285bd" data-element_type="widget"
                     data-settings="{&quot;_animation&quot;:&quot;fadeInUp&quot;,&quot;_animation_delay&quot;:200}"
                     data-widget_type="divider.default">
